#include<stdio.h>
#define MAX 100
int queue[MAX],rear=-1,front=-1;
void enqueue(int value){
    if(rear==MAX-1){
        printf("OVER FLOW!\n");
    }
    else{
        if(front==-1)
           front=0; 
        
        rear++;
        queue[rear]=value;
        printf("%d inserted",value);}
    }

void dequeue(){
    if(front>rear||front==-1){
        printf("EMPTY!");
    }
    else{
        queue[front];
        printf("%d removed",queue[front]);
        front++;
    }
}
void display(){
    if(front>rear||front==-1){
        printf("UNDER FLOW!");
    }
    else{
        for(int i=front;i<=rear;i++){
            printf("%d",queue[i]);
    }
}
}
int main(){
  int choice,value;
  while(1){
      printf("enter the choice enqueue:1\ndequeue:2\ndisplay:3\ndefault:4\nexit:5");
      scanf("%d",&choice);
      switch(choice){
          case 1:
          printf("enter the value");
          scanf("%d",&value);
          enqueue(value);
          break;
          case 2:
          dequeue();
          break;
          case 3:
          display();
          break;
          case 4:
          printf("Invalid");
          break;
          case 5:
          return 0;
          break;
      }
  }
}

